<div class="d-flex justify-content-center py-3">
    {{#listaVacia}}
        <h2>{{listaVacia}}</h2>
        {{/listaVacia}}
    {{^listaVacia}}

    <table class="table">
        <thead>
        <tr>
        {{#keys}}
            <th>{{.}}</th>
            {{/keys}}
            <th></th>
        </tr>
        </thead>
        <tbody>
        {{#usuarios}}
                <tr>
                    <td>{{idUsuario}}</td>
                    <td>{{nombre}}</td>
                    <td>{{dni}}</td>
                    <td>{{mail}}</td>
                    <td>{{rol}}</td>
                    <td><a href ="rechazarVerificacion?idUsuario={{idUsuario}}" class="btn-sm btn-danger">
                        <i class="fa fa-times-circle" style="color:white;font-size:1em"> Rechazar</i></a>
                        <a href ="aceptarVerificacion?idUsuario={{idUsuario}}" class="btn-sm btn-success">
                        <i class="fa fa-check-circle" style="color:white;font-size:1em"> Aceptar</i></a>
                    </td>
                </tr>
            {{/usuarios}}
        </tbody>
    </table>
    {{/listaVacia}}
</div>